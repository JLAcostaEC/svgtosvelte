{
  "name": "@jlacostaec/svgtosvelte",
  "version": "1.1.4",
  "description": "The Best Way to Convert SVG to Svelte 5 Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/JLAcostaEC/svgtosvelte"
  },
  "main": "./dist/src/index.js",
  "type": "module",
  "bin": {
    "svgtosvelte": "./dist/bin.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc && publint",
    "test": "vitest run --test-timeout=10000",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "changeset:version": "changeset version && git add --all",
    "changeset:publish": "changeset publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "keywords": [
    "svelte",
    "svg",
    "svgtosvelte",
    "CLI",
    "convert"
  ],
  "author": {
    "name": "Jorge Acosta",
    "url": "https://github.com/JLAcostaEC"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.28.0",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/node": "^22.10.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "publint": "^0.3.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "commander": "^13.0.0",
    "estree-walker": "^3.0.3",
    "svelte": "^5.16.1",
    "svelte-check": "^4.1.4"
  },
  "packageManager": "pnpm@10.2.0"
}
